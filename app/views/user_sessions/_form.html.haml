= form_tag user_sessions_path, :method => :post do
  .field
    = label_tag :username
    %br
    = text_field_tag :username
  .field
    = label_tag :password
    %br
    = password_field_tag :password
  .field
    = label_tag :remember_me
    = check_box_tag :remember_me
  %br
  .actions
  = submit_tag "Log in"   
    